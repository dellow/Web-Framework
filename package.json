{
  "name": "web-framework",
  "version": "3.8.0",
  "description": "A rapid frontend ['framework', 'wrapper', 'workflow'] for web applications using Gulp, SASS and Webpack.",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.12",
    "git-rev": "^0.2.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.1.3",
    "gulp-coveralls": "^0.1.4",
    "gulp-file-include": "^1.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-nightwatch": "^0.3.2",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.3.2",
    "gulp-standard": "^8.0.2",
    "gulp-uglify": "^2.0.0",
    "imagemin-cli": "^3.0.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "minifier": "^0.7.1",
    "nightwatch": "^0.9.15",
    "phantomjs-prebuilt": "^2.1.14",
    "run-sequence": "^1.2.2",
    "script-loader": "^0.7.0",
    "selenium-server-standalone-jar": "^3.4.0",
    "standard": "^10.0.2",
    "webpack": "^2.6.1",
    "webpack-stream": "^3.2.0",
    "yargs": "^8.0.1"
  },
  "dependencies": {
    "autotrack": "^1.1.0",
    "axios": "*",
    "browsernizr": "*",
    "jquery": "*",
    "navigo": "*",
    "react": "*",
    "react-addons-update": "*",
    "react-dom": "*",
    "script-loader": "*",
    "slick-carousel": "*",
    "sticky-kit": "*",
    "store": "*",
    "turbolinks": "*",
    "underscore": "*",
    "verbal-expressions": "*"
  },
  "standard": {
    "globals": [
      "$"
    ],
    "ignore": [
      "**/__tests__/*.js",
      "*.jsx"
    ]
  },
  "config": {
    "url": "{{dev_url}}",
    "src": "./src/",
    "dist": "./src/dist/",
    "css": {
      "dirDest": "src/dist/css/",
      "dirSCSS": "src/dist/css/scss/",
      "dest": "src/dist/css/build.css",
      "src": "src/dist/css/scss/build.scss"
    },
    "js": {
      "dirApp": "src/dist/js/app/",
      "dirCommon": "src/dist/js/common/",
      "dirDest": "src/dist/js/",
      "dirTests": "src/dist/js/tests/",
      "dest": "src/dist/js/build/"
    }
  },
  "scripts": {
    "server:php": "php -S localhost:8000 -t ./src/",
    "test:unit": "gulp test:unit",
    "test:integration": "gulp test:integration"
  },
  "private": true
}
