{
  "name": "web-framework",
  "version": "3.9.0",
  "description": "A rapid frontend ['framework', 'wrapper', 'workflow'] for web applications using Gulp, SASS and Webpack.",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.12",
    "git-rev": "^0.2.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.4.2",
    "gulp-coveralls": "^0.1.4",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.3.0",
    "gulp-nightwatch": "^0.3.2",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-standard": "^10.0.0",
    "gulp-uglify": "^3.0.0",
    "imagemin-cli": "^3.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "minifier": "^0.8.1",
    "nightwatch": "^0.9.16",
    "phantomjs-prebuilt": "^2.1.14",
    "run-sequence": "^1.2.2",
    "script-loader": "^0.7.0",
    "selenium-server-standalone-jar": "^3.4.0",
    "standard": "^10.0.2",
    "webpack": "^3.0.0",
    "webpack-stream": "^3.2.0",
    "yargs": "^8.0.2"
  },
  "dependencies": {
    "autotrack": "^1.1.0",
    "axios": "^0.16.2",
    "browsernizr": "*",
    "jquery": "*",
    "jquery-pjax": "^2.0.1",
    "navigo": "^5.2.0",
    "react": "^15.6.1",
    "react-addons-update": "^15.6.0",
    "react-dom": "^15.6.1",
    "slick-carousel": "*",
    "sticky-kit": "*",
    "store": "^2.0.12",
    "underscore": "*",
    "verbal-expressions": "*"
  },
  "standard": {
    "globals": [
      "$"
    ],
    "ignore": [
      "**/__tests__/*.js",
      "*.jsx"
    ]
  },
  "config": {
    "url": "{{dev_url}}",
    "css": {
      "fileBuild": "src/dist/css/build.css",
      "fileSrc": "src/dist/css/scss/build.scss",
      "dirBuild": "src/dist/css/",
      "dirSCSS": "src/dist/css/scss/"
    },
    "img": {
      "dirRaw": "src/dist/images/raw/",
      "dirOptimised": "src/dist/images/optimised/"
    },
    "js": {
      "dirApp": "src/dist/js/app/",
      "dirBuild": "src/dist/js/build/",
      "dirCommon": "src/dist/js/common/",
      "dirDest": "src/dist/js/",
      "dirTests": "src/dist/js/tests/"
    }
  },
  "scripts": {
    "server:php": "php -S localhost:8000 -t src/",
    "test:unit": "gulp test:unit",
    "test:integration": "gulp test:integration"
  },
  "private": true
}
