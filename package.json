{
  "name": "web-framework",
  "version": "3.5.0",
  "description": "A rapid frontend ['framework', 'wrapper', 'workflow'] for web applications using SASS, Webpack, Gulp and NPM.",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.16.0",
    "git-rev": "^0.2.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.1.3",
    "gulp-coveralls": "^0.1.4",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nightwatch": "^0.3.2",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.3.2",
    "gulp-standard": "^8.0.2",
    "gulp-uglify": "^2.0.0",
    "imagemin-cli": "^3.0.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "minifier": "^0.7.1",
    "nightwatch": "^0.9.4",
    "phantomjs-prebuilt": "^2.1.14",
    "run-sequence": "^1.2.2",
    "script-loader": "^0.7.0",
    "selenium-server-standalone-jar": "^3.0.1",
    "standard": "^8.1.0",
    "webpack": "^1.13.1",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "autotrack": "*",
    "browsernizr": "*",
    "jquery": "*",
    "navigo": "^4.0.0",
    "react": "^15.2.1",
    "react-addons-update": "15.0.0",
    "react-dom": "^15.2.1",
    "script-loader": "^0.7.0",
    "slick-carousel": "^1.6.0",
    "turbolinks": "^5.0.0",
    "underscore": "*"
  },
  "standard": {
    "globals": [
      "$"
    ],
    "ignore": [
      "**/__tests__/*.js",
      "*.jsx"
    ]
  },
  "config": {
    "url": "{{dev_url}}",
    "src": "./src/",
    "dist": "./src/dist/",
    "css": {
      "dirDest": "src/dist/css/",
      "dirSCSS": "src/dist/css/scss/",
      "dest": "src/dist/css/build.css",
      "src": "src/dist/css/scss/build.scss"
    },
    "js": {
      "dirApp": "src/dist/js/app/",
      "dirCommon": "src/dist/js/common/",
      "dirDest": "src/dist/js/",
      "dirTests": "src/dist/js/tests/",
      "dest": "src/dist/js/build/"
    }
  },
  "scripts": {
    "test:unit": "gulp test:unit",
    "test:integration": "gulp test:integration",
    "sprite": "sprity create $npm_package_config_dist/images/icons/ $npm_package_config_dist/images/icons/png/*.png -c ../images/icons/ -s ../../css/scss/site/_sprites.scss -p sass -n sprite --prefix css-sprite --orientation horizontal --background '#FFFFFF' --margin '2' -d 1:72 -d 2:192"
  },
  "private": true
}
